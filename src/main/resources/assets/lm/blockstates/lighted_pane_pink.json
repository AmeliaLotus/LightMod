{
    "multipart": [
        {   "apply": { "model": "lm:lighted_pane_pink_post" }},
        {   "when": { "north": true },
            "apply": { "model": "lm:lighted_pane_pink_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "lm:lighted_pane_pink_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "lm:lighted_pane_pink_side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "lm:lighted_pane_pink_side_alt", "y": 90 }
        },
        {   "when": { "north": false },
            "apply": { "model": "lm:lighted_pane_pink_noside" }
        },
        {   "when": { "east": false },
            "apply": { "model": "lm:lighted_pane_pink_noside_alt" }
        },
        {   "when": { "south": false },
            "apply": { "model": "lm:lighted_pane_pink_noside_alt", "y": 90 }
        },
        {   "when": { "west": false },
            "apply": { "model": "lm:lighted_pane_pink_noside", "y": 270 }
        }
    ]
}
